   ],
   "source": [
    "def sumList(items):\n",
    "    sumofitems = 0\n",
    "    for x in items:\n",
    "        sumofitems += x\n",
    "    return sumofitems\n",
    "\n",
    "print(sumList([1,3,5,-8,12,-14,53,32,48,-37]))\n"
   ]
